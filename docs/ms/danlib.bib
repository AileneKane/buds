	Keywords = {Biogeochemical hypothesis; C:N ratio; Inner Mongolia; The Tibetan Plateau; Xinjiang},
	Number = {1},
	Pages = {115-122},
	Title = {Stoichiometry and large-scale patterns of leaf carbon and nitrogen in the grasslands of China},
	Type = {Journal Article},
