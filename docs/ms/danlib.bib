	Journal = {New Phytologist},
	Keywords = {foliar nitrogen; functional traits; grassland; leaf mass per area (LMA); photosynthesis; photosynthetic nitrogen use efficiency (PNUE)},
	Note = {WOS:000237353100018},
	Number = {4},
	Pages = {835-848},
	Title = {A test of the generality of leaf trait relationships on the Tibetan Plateau},
	Type = {Journal Article},
