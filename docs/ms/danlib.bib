	Note = {Dorji, Tsechoe Totland, Orjan Moe, Stein R Hopping, Kelly A Pan, Jianbin Klein, Julia A eng Research Support, U.S. Gov't, Non-P.H.S. England 2013/03/19 06:00 Glob Chang Biol. 2013 Feb;19(2):459-72. doi: 10.1111/gcb.12059. Epub 2012 Nov 15.},
	Number = {2},
	Pages = {459-72},
